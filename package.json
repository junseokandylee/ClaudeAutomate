{
  "name": "claude-parallel-runner",
  "version": "2.5.0",
  "description": "Parallel SPEC execution runner for Claude Code CLI",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
